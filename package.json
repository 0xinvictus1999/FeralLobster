{
  "name": "axobase",
  "version": "1.0.0",
  "description": "Axobase - AI Digital Life Autonomous Evolution Platform (Base L2)",
  "main": "dist/cli/axo.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli/axo.ts",
    "export": "tsx src/cli/axo.ts export",
    "deploy": "tsx src/cli/axo.ts deploy",
    "monitor": "tsx src/cli/axo.ts monitor",
    "resurrect": "tsx src/cli/axo.ts resurrect",
    "lineage": "tsx src/cli/axo.ts lineage",
    "test": "vitest",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "test:security": "vitest run test/security",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/ --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "contract:compile": "hardhat compile",
    "contract:test": "hardhat test",
    "contract:deploy": "hardhat run contracts/scripts/deploy.ts",
    "contract:deploy:base": "hardhat run deploy/base/deploy.ts --network base",
    "contract:deploy:sepolia": "hardhat run deploy/base/deploy.ts --network baseSepolia",
    "docker:build": "docker build -t axobase:latest .",
    "rebrand": "node scripts/rebrand.js"
  },
  "keywords": [
    "ai",
    "autonomous",
    "base",
    "blockchain",
    "evolution",
    "x402",
    "arweave"
  ],
  "author": "Axobase",
  "license": "MIT",
  "dependencies": {
    "@libp2p/gossipsub": "^13.0.0",
    "@libp2p/mplex": "^10.0.0",
    "@libp2p/tcp": "^9.0.0",
    "@libp2p/websockets": "^8.0.0",
    "@libp2p/bootstrap": "^10.0.0",
    "@noble/hashes": "^1.4.0",
    "@noble/secp256k1": "^2.1.0",
    "axios": "^1.6.7",
    "bip39": "^3.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1",
    "hdkey": "^2.1.0",
    "js-yaml": "^4.1.0",
    "libp2p": "^1.3.0",
    "merkletreejs": "^0.3.11",
    "openpgp": "^5.11.1",
    "sodium-native": "^4.1.1",
    "tar": "^6.2.0",
    "uuid": "^9.0.1",
    "viem": "^2.7.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/hdkey": "^2.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@types/openpgp": "^4.4.22",
    "@types/tar": "^6.1.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "hardhat": "^2.20.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-viem": "^1.0.0",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "@vitest/coverage-v8": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
