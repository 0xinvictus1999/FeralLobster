# ============================================
# 🔧 Orchestrator API 环境变量配置
# ============================================

# 数据库连接 URL
DATABASE_URL=sqlite:///./feral.db

# Arweave 钱包密钥 (JSON 格式，用于永久存储元数据)
ARWEAVE_KEY={"kty":"RSA","n":"...","e":"..."}

# Akash 网络助记词 (用于部署 AI 代理到去中心化云)
AKASH_MNEMONIC=your_akash_mnemonic_here

# AiNFT API 密钥
AINFT_API_KEY=your_ainft_api_key_here

# 智能合约地址
CONTRACT_ADDRESS=0x...

# Base Sepolia RPC 节点
BASE_SEPOLIA_RPC=https://sepolia.base.org

# 平台钱包私钥 (用于合约交互，仅测试网使用)
PRIVATE_KEY=0x...

# ============================================
# 🔐 可选配置
# ============================================

# API 认证密钥
API_SECRET_KEY=your_api_secret_key_here

# JWT 密钥
JWT_SECRET=your_jwt_secret_here

# Redis 缓存 (可选)
REDIS_URL=redis://localhost:6379/0

# 日志级别
LOG_LEVEL=INFO

# 可选: Sentry DSN
SENTRY_DSN=

# 可选: 邮件服务 (用于通知)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
